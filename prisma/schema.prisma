generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model card {
  card_id   Int       @id @default(autoincrement())
  content   String
  createdat DateTime  @default(now())
  list_id   Int?
  updatedat DateTime?
  list      list?     @relation(fields: [list_id], references: [list_id])
}

model list {
  createdat DateTime  @default(now())
  list_id   Int       @id @default(autoincrement())
  title     String    
  updatedat DateTime?
  card      card[]
}