generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model card {
  card_id   Int       @default(autoincrement()) @id
  content   String
  createdat DateTime  @default(now())
  list_id   Int?
  updatedat DateTime?
  list      list?      @relation(fields: [list_id], references: [list_id])
}

model list {
  createdat DateTime  @default(now())
  list_id   Int       @default(autoincrement()) @id
  title     String
  updatedat DateTime?
  card      card[]
}